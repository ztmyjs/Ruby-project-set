<p id="notice"><%= notice %></p>
<h2><%= @team.name %></h2>
<h3>Members</h3>
<ol>
    <% @team.users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
</ol>
<h3>Presentations</h3>
<ol>
    <% @team.presentations.each do |pres| %>
        <li><%= pres.name %></li>
    <% end %>
</ol>

