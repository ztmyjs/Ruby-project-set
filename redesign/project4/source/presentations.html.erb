---
title: Presentations
---
<div class="content">
  <h3>Presentations</h3>
  <table>
    <tr>
      <th>Title</th>
      <th>Location and Date</th>
    </tr>

    <% data.presentations.presentations.each do |p| %>
      <tr>
        <% if p.link == "" %>
          <td> <%= p.title %> </td>
        <% else %>
          <td> <a href="<%= p.link %>"> <%= p.title %> </a></td>
        <% end %>
        <td> <%= p.when %> </td>
      </tr>
    <% end %>
  </table>
</div>
